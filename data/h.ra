LOAD Student
LOAD Marks
TEMP1 <- SELECT Advisor_id == 2 FROM Student
RENAME Stud_Id TO SID FROM TEMP1
TEMP2 <- CROSS TEMP1 Marks
TEMP3 <- SELECT SID == Stud_Id FROM TEMP2
TEMP4 <- SELECT Maths_marks > 90 FROM TEMP3
OUTPUTH <- PROJECT Stud_Id FROM TEMP4
CLEAR TEMP1
CLEAR TEMP2
CLEAR TEMP3
CLEAR TEMP4
CLEAR Student
CLEAR Marks
PRINT OUTPUTH
