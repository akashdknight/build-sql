LOAD Student
LOAD Marks
RENAME Stud_Id TO SID FROM Student
TEMP1 <- CROSS Student Marks
TEMP2 <- SELECT SID == Stud_Id FROM TEMP1
TEMP3 <- SELECT Maths_marks >= 70 FROM TEMP2
TEMP4 <- SELECT Physics_marks > 60 FROM TEMP3
TEMP5 <- SELECT Stud_age >= 16 FROM TEMP4
TEMP6 <- SELECT Stud_age <= 18 FROM TEMP5
OUTPUTD <- PROJECT Stud_Id, Maths_marks, Physics_marks, Stud_age FROM TEMP6
CLEAR TEMP1
CLEAR TEMP2
CLEAR TEMP3
CLEAR TEMP4
CLEAR TEMP5
CLEAR TEMP6
CLEAR Student
CLEAR Marks
PRINT OUTPUTD
